{{ form_start(form) }}
<div class="form-group">
    <label><i class="fa fa-tag" aria-hidden="true"></i>
        {{ form_label(form.Nombre) }}</label>
    {{ form_widget(form.Nombre) }}
    <small>{{ form_help(form.Nombre) }}</small>
    <div class="form-error">
        <span class="text-danger">{{ form_errors(form.Nombre) }}</span>
    </div>
</div>
<div class="form-group">
    <label><i class="fa fa-building" aria-hidden="true"></i>
        {{ form_label(form.Compania) }} <label>
    {{ form_widget(form.Compania) }}
    <small>{{ form_help(form.Compania) }}</small>
    <div class="form-error">
        <span class="text-danger">{{ form_errors(form.Compania) }}</span>
    </div>
</div>
<div class="form-group">
    <label><i class="fa fa-calendar"></i>
        {{ form_label(form.Fecha_Salida) }}<label>
    {{ form_widget(form.Fecha_Salida) }}
    <small>{{ form_help(form.Fecha_Salida) }}</small>
    <div class="form-error">
        <span class="text-danger">{{ form_errors(form.Fecha_Salida) }}</span>
    </div>
</div>
<button class="btn btn-primary">Guardar</button>
{{ form_end(form) }}
